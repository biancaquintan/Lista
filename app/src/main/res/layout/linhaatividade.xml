<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:layout_width="217dp"
        android:layout_height="35dp"
        android:text="New Text"
        android:id="@+id/tvllvTitulo"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.SearchResult.Title"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="5dp"
        tools:layout_editor_absoluteY="4dp"
        app:layout_constraintRight_toLeftOf="@+id/tvllvDescricao"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="19dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/tvllvDescricao"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/tvllvTitulo"
        android:layout_below="@+id/tvllvTitulo"
        android:layout_marginRight="231dp"
        android:text="New Text"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Small"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_editor_absoluteY="39dp" />

</RelativeLayout>
